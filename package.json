{
  "name": "ksrb.github.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@graphql-codegen/cli": "^1.11.2",
    "@graphql-codegen/fragment-matcher": "^1.12.2",
    "@graphql-codegen/introspection": "^1.11.2",
    "@graphql-codegen/typescript": "^1.11.2",
    "@graphql-codegen/typescript-operations": "^1.11.2",
    "@graphql-codegen/typescript-react-apollo": "^1.11.2",
    "@material-ui/core": "^4.9.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.0",
    "@types/node": "^12.12.25",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-jss": "^10.0.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-utilities": "^1.3.3",
    "clsx": "^1.1.0",
    "concurrently": "^5.0.2",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.1",
    "graphql.macro": "^1.4.2",
    "jss": "^10.0.3",
    "jss-plugin-compose": "^10.0.3",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-jss": "^10.0.3",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "concurrently -r \"npm:start:dev\" \"npm:graphql-codegen:watch\"",
    "start:dev": "react-scripts start",
    "graphql-codegen": "graphql-codegen",
    "graphql-codegen:watch": "npm run graphql-codegen -- -w",
    "format": "prettier --write \"src/**/*.{tsx,graphql}\" \"src/*.{tsx,graphql}\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "ignoreRestSiblings": false
        }
      ],
      "no-unused-expressions": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
